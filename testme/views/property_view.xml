<odoo>
    <record id="property_view_tree" model="ir.ui.view">
        <field name="name">testme.property.tree</field>
        <field name="model">testme.property</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date_available"/>
                <field name="selling_price"/>
                <field name="garden"/>
            </tree>
        </field>

    </record>
    <record id="property_action" model="ir.actions.act_window">
        <field name="name">Property action </field>
        <field name="res_model">testme.property</field>
    </record>
    <menuitem
            id="property_menu_item"
            name="Properties"
            parent="properties_menu"
            action="property_action"
    />
</odoo>